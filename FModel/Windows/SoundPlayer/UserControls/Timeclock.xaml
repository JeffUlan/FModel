<UserControl x:Class="FModel.Windows.SoundPlayer.UserControls.Timeclock"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:visualization="clr-namespace:FModel.Windows.SoundPlayer.Visualization"
             mc:Ignorable="d">
    <Grid>
        <visualization:Timeclock x:Name="NotTimeclock" VerticalAlignment="Top" Padding="10,0,10,5" LabelForeground="#FFEFEFEF" TimeForeground="#FFEFEFEF" Background="#FF1C2026"/>
    </Grid>
</UserControl>
